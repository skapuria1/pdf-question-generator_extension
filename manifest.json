{
  "manifest_version": 3,
  "name": "AI PDF Question Generator",
  "version": "1.0",
  "description": "Extracts text from PDFs and uses AI to generate questions.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["libs/pdf.worker.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
